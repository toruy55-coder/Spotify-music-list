# Copy this file to `.env` and fill in your credentials

# Spotify API credentials (from your Spotify Developer Dashboard)
CLIENT_ID=your_spotify_client_id
CLIENT_SECRET=your_spotify_client_secret
# Redirect URI must match one registered in your Spotify app settings
# e.g. "http://localhost:8888/callback" or "http://127.0.0.1:8888/callback"
REDIRECT_URI=http://localhost:8888/callback

# Optional defaults (can also be overridden via command line)
#PLAYLIST_NAME=MTB Daily Morning
#HISTORY_FILE=history.json
#DAYS_HISTORY=3
#MIN_TRACKS=20
#MAX_TRACKS=40
